{
  "name": "fetchres",
  "version": "0.0.0",
  "main": "lib/fetchres.js",
  "scripts": {
    "clean": "git clean -fxd",
    "files": "find . -name '*.js' ! -path './node_modules/*' ! -path './bower_components/*' ! -path './lib/*'",
    "build": "mkdir -p lib && browserify src/fetchres.js -t babelify --outfile lib/fetchres.js  --standalone fetchres",
    "prepublish": "npm run build",
    "postpublish": "rm -r lib",
    "pretest": "npm run build",
    "test": "jshint `npm run -s files` && lintspaces -i js-comments -e .editorconfig `npm run -s files` && npm run build && mocha"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babelify": "^6.1.3",
    "browserify": "^8.1.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "es6-promise": "^2.0.1",
    "isomorphic-fetch": "^2.0.0",
    "jshint": "^2.5.11",
    "lintspaces-cli": "^0.4.0",
    "mocha": "^2.1.0",
    "nock": "^0.56.0",
    "npm-prepublish": "^1.0.2"
  }
}
